<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sti.edu.reactionrush">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_background"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_background_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ReactionRush">
        <activity
            android:name=".colorText"
            android:noHistory="true"/>
        <activity
            android:name=".changeColor"
            android:noHistory="true" />
        <activity
            android:name=".findColor"
            android:noHistory="true" />
        <activity
            android:name=".findColor2"
            android:noHistory="true" />
        <activity
            android:name=".findColor3"
            android:noHistory="true" />
        <activity
            android:name=".findNumber"
            android:noHistory="true" />
        <activity
            android:name=".findNumber2"
            android:noHistory="true" />
        <activity
            android:name=".findNumber3"
            android:noHistory="true" />
        <activity
            android:name=".findColorRetry"
            android:noHistory="true" />
        <activity
            android:name=".findColorRetry2"
            android:noHistory="true" />
        <activity
            android:name=".findColorRetry3"
            android:noHistory="true" />
        <activity
            android:name=".findColorRetry4"
            android:noHistory="true" />
        <activity
            android:name=".findColorRetry5"
            android:noHistory="true" />
        <activity
            android:name=".findColorRetry6"
            android:noHistory="true" />
        <activity
            android:name=".Menu"
            android:noHistory="true" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>